{
  "name": "keep",
  "version": "1.0.0",
  "description": "a web app generates review plan",
  "config": {
    "qa": ""
  },
  "scripts": {
    "build": "fis3 release prod -u",
    "dev": "fis3 release rd -u -wL",
    "dev-qa": "fis3 release rd-qa -u -wL",
    "build-qa": "fis3 release prod-qa -u",
    "deploy": "fis3 release prod-ol",

    "predev": "gulp images",
    "predeploy": "npm run build-qa",
    "postdeploy": "npm run gen:sw:dist && npm run upload:sw",

    "gen:sw:dist": "node ../build/gen-service-worker.js dist",
    "upload:sw": "node ../build/upload-service-worker.js"
  },
  "keywords": [
    "webapp",
    "pwa",
    "vue2"
  ],
  "author": "adoug",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-import": "^1.1.1",
    "fis-parser-babel-6.x": "^6.24.1",
    "fis-parser-node-sass": "^1.0.1",
    "fis3-deploy-http-push": "^2.0.6",
    "fis3-deploy-local-deliver": "^1.0.0",
    "fis3-deploy-skip-packed": "0.0.5",
    "fis3-hook-commonjs": "^0.1.22",
    "fis3-hook-node_modules": "^2.2.3",
    "fis3-optimizer-minifier": "^1.1.7",
    "fis3-packager-deps-pack": "^0.1.2",
    "fis3-parser-vue-component": "^5.2.0",
    "fis3-postpackager-loader": "^1.5.2",
    "fis3-postprocessor-autoprefixer": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-image-resize": "^0.13.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-rename": "^1.2.2",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-webp": "^4.0.0",
    "loaders.css": "^0.1.2",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "stylelint": "^7.12.0",
    "stylelint-config-sass-guidelines": "^2.1.0",
    "stylelint-config-standard": "^16.0.0",
    "sw-precache": "^5.2.0"
  },
  "dependencies": {
    "base64-js": "^1.2.1",
    "blueimp-md5": "^2.7.0",
    "buffer": "^5.0.6",
    "element-ui": "^1.3.3",
    "es6-promise": "^4.1.0",
    "ieee754": "^1.1.8",
    "is-buffer": "^1.1.5",
    "lodash": "^4.17.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isequal": "^4.5.0",
    "process": "^0.11.10",
    "vue": "^2.3.3",
    "vue-resource": "^1.3.1",
    "vue-router": "^2.5.3",
    "vue-smooth-picker": "^0.2.4",
    "vuedraggable": "^2.14.1",
    "vuex": "^2.3.1"
  }
}
