    "express-jwt": "^5.3.0",
    "express-unless": "^0.3.0",
    "hbs": "~4.0.1",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.10.5",
    "morgan": "~1.7.0",
    "on-finished": "^2.3.0",
    "redis": "^2.7.1",
    "response-time": "^2.3.2",
    "serve-favicon": "~2.3.2"
  },
